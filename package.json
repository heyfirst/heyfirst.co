{
  "name": "heyfirst-co-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i app/styles/tailwind.css -o app/styles/_generated.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:debug": "concurrently \"npm run dev:css\" \"remix dev --debug\"",
    "dev:css": "tailwindcss -w -i app/styles/tailwind.css -o app/styles/_generated.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "1.11.1",
    "@remix-run/react": "1.11.1",
    "@remix-run/serve": "1.11.1",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "fathom-client": "3.5.0",
    "feed": "4.2.2",
    "fuse.js": "6.6.2",
    "giscus": "1.2.6",
    "gray-matter": "4.0.3",
    "highlight.js": "11.7.0",
    "isbot": "3.6.5",
    "mdx-bundler": "9.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-highlight": "6.0.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "3.0.1",
    "remark-mdx-images": "2.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.11.1",
    "@remix-run/eslint-config": "1.11.1",
    "@tailwindcss/typography": "0.5.9",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "concurrently": "7.6.0",
    "eslint": "8.32.0",
    "eslint-plugin-tailwindcss": "3.8.2",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": "18"
  }
}
